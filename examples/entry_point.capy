
// run with `capy entry_point.capy --entry-point _start`
_start :: () -> {
    printf("_start\n", 0);

    code := main();
    
    printf("main() returned with: %i\n", code);
    exit(505);
};

main :: () -> i64 {
    printf("Hello from Main!\n", 0);

    0
};

printf :: (str: string, i: i64) -> extern;

exit :: (code: i32) -> extern;