// run with `capy run examples/entry_point.capy --entry-point _start`

core :: mod "core";

_start :: () {
    printf("_start\n", 0);

    code := main();
    
    printf("main() returned with: %i\n", code);
    exit(255);
}

main :: () -> i64 {
    core.println("Hello from Main!");

    0
}

printf :: (text: str, i: i64) extern;

exit :: (code: u8) extern;
